{
    "name": "sqlops-mssql-spatial-preview",
    "displayName": "MSSQL Spatial Preview",
    "description": "SqlOps Studio MSSQL Spatial Preview Extension",
    "version": "0.0.1",
    "publisher": "FlexArts",
    "engines": {
        "vscode": "^1.29.0",
        "sqlops": "*"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:sql",
        "onCommand:extension.showCurrentConnection"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.showCurrentConnection",
                "title": "Show Current Connection"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "build": "webpack --config webpack.config.js --mode production",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install && node ./node_modules/sqlops/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/ol": "4.6.2",
        "sqlops": "github:anthonydresser/sqlops-extension-sqlops",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "webpack": "^4.26.1",
        "webpack-cli": "3.1.2"
    },
    "dependencies": {
        "ol": "5.3.0"
    }
}
