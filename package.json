{
    "name": "sqlops-mssql-spatial-preview",
    "displayName": "MSSQL Spatial Preview",
    "description": "SqlOps Studio MSSQL Spatial Preview Extension",
    "version": "0.0.1",
    "publisher": "FlexArts",
    "repository": {
        "type": "git",
        "url": "https://github.com/flexarts/sqlops-mssql-spatial-preview.git"
    },
    "engines": {
        "vscode": "^1.28.0",
        "sqlops": "*"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.sayHello",
        "onLanguage:sql"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.sayHello",
                "title": "Hello World"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install && node ./node_modules/sqlops/bin/install",
        "test": "node ./node_modules/vscode/bin/test",
        "build": "vsce package --yarn"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42",
        "sqlops": "github:anthonydresser/sqlops-extension-sqlops"
    }
}